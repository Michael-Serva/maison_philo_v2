{% extends 'base.html.twig' %}
{% block title %}
	{% trans %}Category{% endtrans %}
{% endblock %}
{% block body %}
	<h1>{% trans %}Category{% endtrans %}</h1>
	<table class="table">
		<tbody>
			<tr>
				<th>Id</th>
				<td>{{ category.id }}</td>
			</tr>
			<tr>
				<th>{% trans %}Title{% endtrans %}</th>
				<td>{{ category.title }}</td>
			</tr>
		</tbody>
	</table>
	<a href="{{ path('app_category_index') }}">{% trans %}back to list{% endtrans %}</a>
	<a href="{{ path('app_category_edit', {'id': category.id}) }}">
		{% trans %}edit{% endtrans %}
	</a> {{ include('category/_delete_form.html.twig') }}
{% endblock %}
