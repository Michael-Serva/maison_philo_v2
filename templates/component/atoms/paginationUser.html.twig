<nav aria-label="...">
    <ul class="pagination justify-content-center">
        <li class="page-item {{ users.page <= 1 ? 'disabled' : ''}} ">
            <a class="page-link" href="?page={{users.page - 1}}">
                {% trans %}Previous{% endtrans %}
            </a>
        </li>
        {% for i in 1.. usersTotalPage %}
            <li class="page-item {{users.page == i ? 'disabled' : ''}}">
                <a class="page-link" href="?page={{i}}">{{i}}</a>
            </li>
        {% endfor %}
        <li class="page-item  {{users.page == usersTotalPage ? 'disabled' : ''}}">
            <a class="page-link" href="?page={{users.page + 1}}">
                {% trans %}Next{% endtrans %}
            </a>
        </li>
    </ul>
</nav>
